{
  "name": "sample",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "sample",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.min.js","script.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources":["style.css"],
  "permissions": ["http://*/*"]
}
